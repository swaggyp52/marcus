{
  "typeCheckingMode": "standard",
  "pythonVersion": "3.12",
  "include": ["marcus_app", "marcus_v052", "scripts", "tests"],
  "exclude": ["venv", "build", "dist", ".venv"],
  "extraPaths": [".", "./marcus_app", "./marcus_v052"],
  "venvPath": ".",
  "venv": ".venv",
  "reportMissingImports": true,
  "reportMissingModuleSource": false,
  "reportMissingTypeStubs": false,
  "reportOptionalMemberAccess": true,
  "reportOptionalSubscript": true,
  "reportOptionalOperand": false,
  "reportOptionalCall": true,
  "reportOptionalIterable": true,
  "reportOptionalContextManager": true,
  "reportOptionalArgumentType": true,
  "reportOptionalReturnType": true,
  "reportOptionalAssignment": true,
  "reportGeneralTypeIssues": true,
  "reportReturnType": true,
  "reportUnusedImport": "information",
  "reportUnusedVariable": "information",
  "reportIncompatibleMethodOverride": true,
  "reportIncompatibleVariableOverride": true,
  "reportInconsistentConstructor": true,
  "reportUnusedClass": "information",
  "reportUnusedFunction": "information",
  "reportUnusedParameter": "information",
  "reportUnnecessaryIsInstance": "information",
  "reportUnnecessaryCast": "information",
  "reportUnnecessaryComparison": "information",
  "reportConstantRedefinition": "information",
  "reportInvalidTypeForm": true,
  "reportMatchNotExhaustive": "warning",
  "reportUnusedCallResult": false
}
